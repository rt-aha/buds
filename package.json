{
  "name": "p-bud",
  "version": "1.1.13",
  "description": "用來初始化各種模板的專案",
  "main": "init.js",
  "scripts": {
    "commit": "node commit.js",
    "release": "standard-version && git push --follow-tags origin master",
    "lint:style": "stylelint src/**/*.scss src/**/*.css --syntax scss --fix",
    "lint:prettier": "prettier --write .",
    "lint:js.ts": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/silentice1534/p-bud.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/silentice1534/p-bud/issues"
  },
  "homepage": "https://gitlab.com/silentice1534/p-bud#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.1",
    "commitlint-config-gitmoji": "^2.2.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "4.3.8",
    "inquirer": "^8.0.0",
    "js-yaml": "^4.1.0",
    "jsonfile": "^6.1.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "shelljs": "^0.8.4",
    "standard-version": "^9.2.0",
    "stylelint": "^13.13.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "typescript": "^4.2.4",
    "write-json-file": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{scss,css}": [
      "yarn lint:style"
    ],
    "*": [
      "yarn lint:prettier"
    ],
    "*.{js,ts}": [
      "yarn lint:js.ts"
    ],
    "*.vue": [
      "vue-cli-service lint"
    ]
  }
}
